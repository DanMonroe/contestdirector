
    <div>
        <h3 style="display: inline">Contests</h3>
        {{#paper-button icon-button=true action="transitionTo" param="contests.new"}}{{paper-icon icon="add"}}{{/paper-button}}
    </div>

    <table>
        <thead>
        <tr>
            <th>Contest</th>
            <th></th>
            <th></th>
        </tr>
        </thead>
        <tbody>
        {{#each model as |contest|}}
            <tr>
                <td>{{link-to contest.name "contests.show" contest}}</td>
                <td>{{contest.aircraftType.name}}</td>
                <td>
                    {{#paper-button icon-button=true action="editContest" param=contest}}{{paper-icon icon="edit"}}{{/paper-button}}
                    {{#paper-button icon-button=true action="deleteContest" param=contest}}{{paper-icon icon="delete"}}{{/paper-button}}
                </td>
            </tr>
        {{/each}}
        </tbody>
    </table>





