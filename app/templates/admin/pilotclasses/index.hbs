
<div>
    <h3 style="display: inline">Pilot Classes?</h3>
    {{#paper-button icon-button=true action="transitionTo" param="admin.pilotclasses.new"}}{{paper-icon icon="add"}}{{/paper-button}}
</div>
<table>
    <thead>
    <tr>
        <th>Pilot Class</th>
        <th></th>
    </tr>
    </thead>
    <tbody>
    {{#each model as |pilotclass|}}
        <tr>
            <td>{{link-to pilotclass.name "admin.pilotclasses.show" pilotclass}}</td>
            <td class="iconBar">
                {{#paper-button icon-button=true action="editPilotclass" param=pilotclass}}{{paper-icon icon="edit"}}{{/paper-button}}
                {{#paper-button icon-button=true action="deletePilotclass" param=pilotclass}}{{paper-icon icon="delete"}}{{/paper-button}}
            </td>
        </tr>
    {{/each}}
    </tbody>
</table>





