
{{#paper-content classNames="md-whiteframe-z1"}}
    <h3>Aircraft Types</h3>
    {{#paper-button icon-button=true action="transitionTo" param="admin.aircrafttypes.new"}}Add{{/paper-button}}
    {{#paper-list}}
        {{log aircraftTypes}}
        {{#each model as |aircrafttype|}}
            {{#paper-item class="md-3-line"}}
                <div class="md-list-item-text">
                    <h3>{{#link-to 'admin.aircrafttypes.pilotclasses' aircrafttype}}{{aircrafttype.name}}{{/link-to}}</h3>
                </div>
            {{/paper-item}}
        {{/each}}
    {{/paper-list}}


{{/paper-content}}
{{outlet}}

