<h3>Aircraft Types</h3>

{{#paper-content classNames="md-whiteframe-z1 list-demo"}}
    {{#paper-list}}
        {{#each model as |aircrafttype|}}
            {{#paper-item class="md-3-line"}}
                <div class="md-list-item-text">
                    <h3>{{aircrafttype.name}}</h3>
                </div>
            {{/paper-item}}
        {{/each}}
    {{/paper-list}}
{{/paper-content}}

