<h3>Pilot Classes</h3>

{{#paper-content classNames="md-whiteframe-z1 list-demo"}}
    {{#paper-list}}
        {{#each model as |pilotclass|}}
            {{#paper-item class="md-3-line"}}
                <div class="md-list-item-text">
                    <h3>{{pilotclass.name}}</h3>
                </div>
            {{/paper-item}}
        {{/each}}
    {{/paper-list}}
{{/paper-content}}

