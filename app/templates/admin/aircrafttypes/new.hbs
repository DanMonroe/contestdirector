{{#paper-content}}
    <p>
    <h2>Add a New Aircraft Type</h2>
    </p>
    {{paper-divider}}
    <p>
        <form>
            <h3>{{errorMessage}}</h3>
    <p>
        {{paper-input label="Name" value=model.name required=true required-errortext="Aircraft Type name is required."}}
    </p>
    <p>
        {{#paper-button raised=true action="createAircraftType"}}Save{{/paper-button}}
        {{#paper-button raised=true action="cancelAircraftType"}}Cancel{{/paper-button}}
    </p>
    </form>
    </p>
{{/paper-content}}
