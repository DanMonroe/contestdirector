
{{#paper-content classNames="md-whiteframe-z1"}}
    <h3>{{model.name}} Pilot Classes</h3>
    {{#link-to "admin.aircrafttypes.pilotclasses.new" model}}Add{{/link-to}}

    {{#paper-list}}
        {{#each model.pilotclasses as |pilotclass|}}
            {{#paper-item class="md-3-line"}}
                <div class="md-list-item-text">
                    <h3>{{pilotclass.name}}</h3>
                </div>
            {{/paper-item}}
        {{/each}}
    {{/paper-list}}

{{/paper-content}}
<{{outlet}}>

